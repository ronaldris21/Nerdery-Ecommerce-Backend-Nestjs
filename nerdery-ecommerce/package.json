    "graphql": "^16.10.0",
    "graphql-tools": "^9.0.8",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "streamifier": "^0.1.1",
    "stripe": "^17.5.0",
    "swagger-ui-express": "^5.0.1"
  },
